nav.navbar.navbar-expand-md.navbar-dark.bg-dark.mb-4
    a.navbar-brand(href='/') GTM Templates
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarCollapse' aria-controls='navbarCollapse' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #navbarCollapse.collapse.navbar-collapse
      ul.navbar-nav.mr-auto
        li.nav-item.active
          a.nav-link(href='/')&attributes({'data-ga-action':'event','data-ga-event': {category:'cat',action:'act',label:'label',value:undefined,nonint:0}})
            | Home
            span.sr-only (current)
        li.nav-item
          a.nav-link(href='/categories')&attributes({'data-product-id': template.id,'data-product-name': template.name,'data-product-category': template.category}) Categories
      form.form-inline.mt-2.mt-md-0(action='/search')
        input.form-control.mr-sm-2(name='q' type='text' placeholder='Search' aria-label='Search')
        button.btn.btn-outline-success.my-2.my-sm-0(type='submit' id='search-submit') Search
